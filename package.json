{
  "name": "sign-language-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --configuration=production --base-href=/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/sign-language-playground/stats.json",
    "pwa:assets": "pwa-asset-generator src/assets/icons/dark-logo.svg src/assets/icons/generated/ -i src/index.html -m src/manifest.webmanifest --background white --favicon --mstile && pwa-asset-generator src/assets/icons/light-logo.svg --dark-mode src/assets/icons/generated/ -i src/index.html --background black --splash-only",
    "i18n:find": "transloco-keys-manager find",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "~13.1.0",
    "@angular/material-experimental": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@angular/service-worker": "~13.1.0",
    "@google/model-viewer": "^1.9.2",
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "@mediapipe/holistic": "^0.5.1635989137",
    "@ngneat/transloco": "^3.1.1",
    "@ngxs/store": "^3.7.3",
    "@sutton-signwriting/font-ttf": "^1.2.0",
    "@sutton-signwriting/sgnw-components": "0.10.0",
    "@tensorflow/tfjs": "3.12.0",
    "@tensorflow/tfjs-backend-webgl": "3.12.0",
    "@tensorflow/tfjs-converter": "3.12.0",
    "@tensorflow/tfjs-core": "3.12.0",
    "@tensorflow/tfjs-layers": "3.12.0",
    "cld3-asm": "^3.1.1",
    "comlink": "^4.3.1",
    "flag-icon-css": "^4.1.6",
    "material-components-web": "14.0.0-canary.1af7c1c4a.0",
    "ngx-flag-icon-css": "^1.0.1",
    "pose-viewer": "^0.2.3",
    "rxjs": "~7.4.0",
    "seedrandom": "^2.4.3",
    "stats.js": "^0.17.0",
    "three": "0.125.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1301.0",
    "@angular-devkit/build-angular": "^13.1.0",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.0",
    "@angular/compiler-cli": "~13.1.0",
    "@ngneat/transloco-keys-manager": "^3.2.0",
    "@types/dom-speech-recognition": "^0.0.0",
    "@types/dom-webcodecs": "^0.1.3",
    "@types/jasmine": "~3.10.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/three": "^0.129.1",
    "@types/webgl2": "0.0.6",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "eslint": "^8.4.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^8.2.0",
    "inquirer-autocomplete-prompt": "^1.1.0",
    "jasmine-core": "~3.10.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "open": "^8.0.9",
    "protractor": "~7.0.0",
    "pwa-asset-generator": "^6.0.5",
    "sitemap": "^7.0.0",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.3",
    "webpack-bundle-analyzer": "^4.4.2"
  }
