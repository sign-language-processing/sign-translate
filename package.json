{
  "name": "sign-translate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --configuration=production --base-href=/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/sign-translate/browser/stats.json",
    "pwa:assets": "pwa-asset-generator src/assets/icons/dark-logo.svg src/assets/icons/generated/ -i src/index.html -m src/manifest.webmanifest --background white --favicon --mstile && pwa-asset-generator src/assets/icons/light-logo.svg --dark-mode src/assets/icons/generated/ -i src/index.html --background black --splash-only",
    "assets": "capacitor-assets generate --verbose --ios --android --assetPath src/assets/icons --iconBackgroundColor=white --iconBackgroundColorDark=black --splashBackgroundColor=white --splashBackgroundColorDark=black",
    "i18n:find": "transloco-keys-manager find",
    "test": "ng test --code-coverage",
    "test:chrome": "ng test --browsers=Chrome --code-coverage",
    "test:ci": "ng test --browsers=ChromeHeadless,FirefoxHeadless --watch=false --code-coverage",
    "deploy": "npm run build && node tools/sitemap-generator.js && firebase deploy --only=hosting",
    "mobile:sync": "tsx tools/mobile/sync.ts",
    "mobile:metadata": "tsx tools/mobile/metadata.ts",
    "prepare:husky": "cp -r ./node_modules/@sign/configuration/.husky/ .husky/",
    "prepare": "husky install && npm run prepare:husky",
    "dev:ssr": "ng run sign-translate:serve-ssr",
    "serve:ssr": "node dist/sign-translate/server/main.js",
    "build:ssr": "ng build && ng run sign-translate:server",
    "prerender": "ng run sign-translate:prerender:production --routes /"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.4",
    "@angular/cdk": "^14.0.4",
    "@angular/common": "^14.0.4",
    "@angular/compiler": "^14.0.4",
    "@angular/core": "^14.0.4",
    "@angular/forms": "^14.0.4",
    "@angular/material": "^14.0.4",
    "@angular/platform-browser": "^14.0.4",
    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/platform-server": "^14.0.4",
    "@angular/router": "^14.0.4",
    "@angular/service-worker": "^14.0.4",
    "@capacitor-firebase/analytics": "^0.5.0",
    "@capacitor-firebase/app": "^0.5.0",
    "@capacitor-firebase/crashlytics": "^0.5.0",
    "@capacitor-firebase/performance": "^0.5.0",
    "@capacitor/android": "4.0.0-beta.1",
    "@capacitor/core": "4.0.0-beta.1",
    "@capacitor/filesystem": "^1.1.0",
    "@capacitor/ios": "4.0.0-beta.1",
    "@capacitor/keyboard": "^1.2.3",
    "@capacitor/share": "^1.1.2",
    "@capacitor/splash-screen": "^1.2.2",
    "@google/model-viewer": "^1.12.0",
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "@mediapipe/holistic": "^0.5.1635989137",
    "@ngneat/transloco": "^4.1.0",
    "@nguniversal/express-engine": "^14.0.2",
    "@ngxs/store": "3.7.3",
    "@sign/i18n": "git://github.com/sign/i18n.git",
    "@sutton-signwriting/font-ttf": "^1.3.2",
    "@sutton-signwriting/sgnw-components": "0.12.0",
    "@tensorflow/tfjs": "3.18.0",
    "@tensorflow/tfjs-backend-wasm": "3.18.0",
    "@tensorflow/tfjs-backend-webgl": "3.18.0",
    "@tensorflow/tfjs-backend-webgpu": "0.0.1-alpha.11",
    "@tensorflow/tfjs-converter": "3.18.0",
    "@tensorflow/tfjs-core": "3.18.0",
    "@tensorflow/tfjs-layers": "3.18.0",
    "cld3-asm": "^3.1.1",
    "comlink": "^4.3.1",
    "firebase": "^9.8.4",
    "flag-icons": "^6.6.2",
    "pose-viewer": "^0.5.0",
    "rxjs": "~7.5.5",
    "stats.js": "^0.17.0",
    "three": "^0.142.0",
    "tslib": "^2.4.0",
    "web-vitals": "^2.1.4",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1400.4",
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^14.0.4",
    "@angular/compiler-cli": "^14.0.4",
    "@capacitor/assets": "^1.0.0-next.5",
    "@capacitor/cli": "4.0.0-beta.1",
    "@ngneat/transloco-keys-manager": "^3.4.1",
    "@nguniversal/builders": "^14.0.2",
    "@playwright/test": "^1.23.1",
    "@sign/configuration": "git://github.com/sign/.github.git",
    "@trapezedev/project": "^3.0.5",
    "@types/dom-mediacapture-transform": "^0.1.3",
    "@types/dom-speech-recognition": "^0.0.1",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/three": "^0.141.0",
    "@types/web-app-manifest": "^1.0.2",
    "@types/webgl2": "0.0.6",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.18.0",
    "fuzzy": "^0.1.3",
    "husky": "^8.0.1",
    "inquirer": "^9.0.0",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "jasmine-axe": "^1.1.0",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "^0.0.34",
    "lint-staged": "^13.0.3",
    "open": "^8.4.0",
    "protractor": "~7.0.0",
    "pwa-asset-generator": "^6.1.0",
    "sitemap": "^7.1.1",
    "tiny-async-pool": "^2.1.0",
    "ts-node": "~10.8.1",
    "tsx": "^3.6.0",
    "typescript": "~4.7.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "zod": "^3.17.3"
  }
}
